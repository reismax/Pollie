{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "env": {
    "CONGRESS_API_KEY": {
      "description": "Congress.gov API key from https://api.data.gov",
